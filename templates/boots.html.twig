        {{ msg }}
    </h2>
{% endfor %}
{% for msg in app.session.flashbag.get('erreur1') %}
    <h2 class="alert alert-danger">
        {{ msg }}
    </h2>
{% endfor %}
{% for msg in app.session.flashbag.get('erreur2') %}
    <h2 class="alert  alert-danger">
        {{ msg }}
    </h2>
{% endfor %}

{% for msg in app.session.flashbag.get('alerte3') %}
    <h2 class="alert alert-success">
        {{ msg }}
    </h2>
{% endfor %}
{% for msg in app.session.flashbag.get('erreur3') %}
